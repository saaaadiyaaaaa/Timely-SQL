<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqltodo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/timely_final"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Timer"/>
        <activity android:name=".Activities.EditTask" />
        <activity android:name=".Profile" />
        <activity android:name=".Activities.ViewTask" />
        <activity
            android:name=".Activities.TaskCard"
            android:label="@string/title_activity_task_card"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.AddTask" />
        <activity android:name=".Activities.CalendarCard" />
        <activity android:name=".Activities.ReportCard" />
        <activity android:name=".Dashboard" />
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>